{"ast":null,"code":"import axios from 'axios';\nexport default {\n  data() {\n    return {\n      login: \"\",\n      password: \"\",\n      last_name: \"\",\n      first_name: \"\"\n    };\n  },\n  methods: {\n    Register: function () {\n      axios.post('http://localhost:8000/api/register', {\n        email: this.login,\n        password: this.password,\n        last_name: this.last_name,\n        first_name: this.first_name,\n        is_admin: 0\n      }).then(function () {\n        window.location.replace(\"/\");\n      }).catch(function (error) {\n        alert(error);\n      });\n    }\n  }\n};","map":{"version":3,"mappings":"AAsEA;AAEA;EACAA;IACA;MACAC;MACAC;MACAC;MACAC;IAEA;EACA;EACAC;IACAC;MACAC;QACAC;QACAN;QACAC;QACAC;QACAK;MACA;QACAC;MACA;QACAC;MACA,EACA;IACA;EACA;AACA","names":["data","login","password","last_name","first_name","methods","Register","axios","email","is_admin","window","alert"],"sourceRoot":"src/views","sources":["RegisterView.vue"],"sourcesContent":["<template>\r\n  <body>\r\n      <div class=\"menu-logo\">\r\n        <img src=\"logo.png\" alt=\"menu-logo\">\r\n      </div>\r\n      <div class=\"menu-bar\">\r\n        <ul>\r\n            <li><a href=\"/?\">Home</a></li>\r\n            <li><a href=\"#\">My lesson plan</a></li>\r\n            <li><a href=\"#\">Extra tasks</a></li>\r\n            <li><a href=\"#\">Contact</a></li>\r\n        </ul>\r\n      </div>\r\n      <div class=\"menu-buttons\">\r\n      \r\n        <button><a href=\"/?\">Log in</a></button>\r\n      </div>\r\n      <div class=\"container\">\r\n        <form>\r\n          <h1 style=\"color: black; font-family: Garamond, sans-serif; font-size: 36px!important; font-weight: 700!important; text-shadow: 2px 2px #ddd; \r\n        letter-spacing: 2px; margin-bottom: 50px!important; margin-top: 25px!important; text-align: center!important;\">Registration form</h1>\r\n          <div class=\"form-group\">\r\n            <label for=\"first-name\">First Name:</label>\r\n            <input type=\"text\" name=\"first-name\" id=\"first-name\" placeholder=\"Enter your first name\" required>\r\n          </div>\r\n          <div class=\"form-group\">\r\n            <label for=\"last-name\">Last Name:</label>\r\n            <input type=\"text\" name=\"last-name\" id=\"last-name\" placeholder=\"Enter your last name\" required>\r\n          </div>\r\n          <div class=\"form-group\">\r\n            <label for=\"email\">Email:</label>\r\n            <input type=\"email\" name=\"email\" id=\"email\" placeholder=\"Enter your email address\" required>\r\n          </div>\r\n          <div class=\"form-group\">\r\n            <label for=\"password\">Password:</label>\r\n            <input type=\"password\" name=\"password\" id=\"password\" placeholder=\"Enter your password\" required>\r\n          </div>\r\n          <div class=\"form-group\">\r\n            <label for=\"confirm-password\">Confirm Password:</label>\r\n            <input type=\"password\" name=\"confirm-password\" id=\"confirm-password\" placeholder=\"Confirm your password\" required>\r\n          </div>\r\n          <div class=\"form-group\">\r\n            <label for=\"birthdate\">Date of Birth:</label>\r\n            <input type=\"date\" name=\"birthdate\" id=\"birthdate\">\r\n          </div>\r\n          <div class=\"form-group\">\r\n            <label for=\"gender\">Gender:</label>\r\n            <select name=\"gender\" id=\"gender\">\r\n              <option value=\"male\">Male</option>\r\n              <option value=\"female\">Female</option>\r\n              <option value=\"non-binary\">Non-binary</option>\r\n            </select>\r\n          </div>\r\n\r\n          <div class=\"form-group\">\r\n            <a href=\"/mocks\"><button @click=\"Register()\">\r\n           \r\n            <span class=\"transition\"></span>\r\n            <span class=\"gradient\"></span>\r\n            <span class=\"btn\">Create account</span>\r\n      \r\n            </button></a>\r\n        </div>\r\n        \r\n      </form>\r\n    </div>\r\n  </body>\r\n</template>\r\n  \r\n  <script>\r\n  import axios from 'axios'\r\n  \r\n  export default {\r\n    data() {\r\n          return {\r\n              login: \"\",\r\n              password: \"\",\r\n              last_name: \"\",\r\n              first_name: \"\",\r\n\r\n          }\r\n      },\r\n    methods: {\r\n      Register: function () {\r\n        axios.post('http://localhost:8000/api/register', {\r\n              email: this.login,\r\n              password: this.password,\r\n              last_name: this.last_name,\r\n              first_name: this.first_name,\r\n              is_admin: 0\r\n            }) .then(function () {\r\n                window.location.replace(\"/\");\r\n          }).catch(function (error) {\r\n            alert(error)\r\n          }\r\n          )\r\n      }\r\n    }\r\n  }\r\n  </script>\r\n  \r\n  <style lang=\"sass\" scoped>\r\n  body\r\n    \r\n    \r\n    background-size: cover\r\n    background-repeat: no-repeat\r\n    background-attachment: fixed\r\n    height: 140px\r\n   \r\n    display: flex\r\n    \r\n    justify-content: space-between\r\n    padding: 0 20px\r\n    .menu-logo\r\n      display: flex\r\n      flex-grow: 1\r\n\r\n    .menu-logo img\r\n      height: 120px\r\n      width: auto\r\n      position: relative\r\n      z-index: 9999\r\n      visibility: visible\r\n    .menu-bar\r\n      background-color: #ffffff\r\n      width: 100%\r\n      height: 140px\r\n      display: flex\r\n      justify-content: center\r\n      align-items: center\r\n      position: fixed\r\n      text-align: center\r\n      padding: 10px \r\n    .menu-bar ul\r\n        list-style: none\r\n        margin: 0\r\n        padding: 0\r\n        text-decoration: none   \r\n    .menu-bar li\r\n        display: inline-block\r\n        margin: 0 50px\r\n\r\n\r\n    .menu-bar a\r\n        color: black\r\n        font-family: \"Garamond\", serif\r\n        font-size: 28px\r\n        font-weight: bold\r\n        text-decoration: none   \r\n        \r\n    \r\n    .menu-buttons \r\n      flex: 0 0 auto\r\n      display: flex\r\n      align-items: center\r\n      text-decoration: none\r\n      \r\n\r\n\r\n    .menu-buttons button\r\n      background-color: black\r\n      border: 2px solid black\r\n      color: white\r\n      font-size: 36px\r\n      padding: 10px 20px\r\n      margin-left: 20px\r\n      cursor: pointer\r\n      font-family: \"Garamond\", serif\r\n      margin-top: -15px\r\n      font-weight: bold\r\n      text-decoration: none\r\n    .menu-buttons button a:visited\r\n      color: white\r\n      text-decoration: none\r\n      \r\n    \r\n\r\n\r\n  .container\r\n    background-color: white\r\n    border-radius: 10px\r\n    box-shadow: 0 14px 28px rgba(0, 0, 0, 0.25), 0 10px 10px rgba(0, 0, 0, 0.22)\r\n    padding: 40px\r\n    width: 80%\r\n    max-width: 700px\r\n    text-align: center\r\n    transition: all 0.3s cubic-bezier(0.25, 0.8, 0.25, 1)\r\n    display: inline-block\r\n    margin: 0 auto\r\n    position: absolute\r\n    top: 50%\r\n    left: 50%\r\n    transform: translate(-50%, -50%)\r\n    margin-top: 50px\r\n\r\n  .container:hover\r\n    box-shadow: 0 30px 38px rgba(0,35, 156, 0.3), 0 30px 12px rgba(0,35, 156, 0.22)\r\n    align-content: center\r\n    align-items: center\r\n    align-self: center\r\n\r\n  .form-group\r\n    margin-bottom: 20px\r\n    align-self: center\r\n    align-items: center\r\n    text-align: center\r\n    display: flex\r\n    justify-content: center\r\n    color: white\r\n    margin-right: 30px\r\n  \r\n  label\r\n  \r\n    font-size: 20px\r\n    margin-bottom: 5px\r\n    font-weight: bold\r\n    display: block\r\n\r\n  \r\n\r\n\r\n  input[type=\"text\"],\r\n  input[type=\"email\"],\r\n  input[type=\"password\"],\r\n  input[name=\"last-name\"],\r\n  input[name=\"first-name\"],\r\n  input[type=\"data\"],\r\n  \r\n  select\r\n    border: 1px solid #dadce0\r\n    border-radius: 4px\r\n    box-shadow: 0 0 0 transparent\r\n    font-size: 28px\r\n    line-height: 30px\r\n    outline: transparent\r\n    padding: 11px 16px\r\n    transition: border .3s,box-shadow .3s\r\n    width: 100%\r\n    box-sizing: border-box\r\n   \r\n\r\n  input[type=\"text\"]:focus,\r\n  input[type=\"email\"]:focus,\r\n  input[type=\"password\"]:focus,\r\n  input[name='last-name']:focus,\r\n  input[name='first-name']:focus,\r\n  select:focus\r\n    border: 2px solid #00239c!important\r\n    padding: 11px 16px\r\n    transition: border .3s,box-shadow .3s\r\n    width: 100%\r\n    box-sizing: border-box\r\n\r\n  button\r\n\r\n    font-family: \"Garamond\", serif\r\n    font-size: 36px\r\n    font-weight: 500\r\n    background: #1F2937\r\n    color: white\r\n    border: none\r\n    position: relative\r\n    overflow: hidden\r\n    border-radius: 4px\r\n    padding: 11px 100px\r\n    button-height: 30px\r\n    margin-top: 50px\r\n    cursor: pointer\r\n    font-weight: bold\r\n\r\n  .gradient\r\n    position: absolute\r\n    width: 100%\r\n    height: 100%\r\n    left: 0\r\n    top: 0\r\n    border-radius: 4px\r\n    margin-top: -0.25em\r\n    background-image: linear-gradient(rgba(0, 0, 0, 0), rgba(0, 0, 0, 0), rgba(0, 0, 0, 0.3))\r\n\r\n  .btn\r\n    position: relative\r\n    top: -1px\r\n    \r\n\r\n  .transition\r\n    transition-timing-function: cubic-bezier(0, 0, 0.2, 1)\r\n    transition-duration: 500ms\r\n    background-color: rgba(0, 0, 0, 1.0)\r\n    border-radius: 9999px\r\n    width: 0\r\n    height: 0\r\n    position: absolute\r\n    left: 50%\r\n    top: 50%\r\n    transform: translate(-50%, -50%)\r\n\r\n  button:hover .transition\r\n    width: 14em\r\n    height: 14em\r\n\r\n  button:active\r\n    transform: scale(0.97)\r\n  </style>\r\n  \r\n \r\n  "]},"metadata":{},"sourceType":"module","externalDependencies":[]}